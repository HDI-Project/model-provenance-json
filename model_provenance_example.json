{
    "label_spec": {
        "binarize": ">0 & <1",
        "window": "56 days",
        "offset": "7 days",
        "min_training_data": "28 days",
        "lead": "28 days",
        "label_function": {"python": "lambda x: x['Price'] - x['Cost']"},
        "reduce": "sum"
    },
    "data_splits": [{
        "id": "train",
        "start_time": "2014/01/01",
        "end_time": "2014/06/01",
        "sampling": {
            "strategy": "random",
            "by": "Order ID",
            "n": 10,
            "gap": "3 days"
        }

    },
    {
        "id": "thresh-tune",
        "start_time": "2014/06/02",
        "end_time": "2015/01/01",
        "sampling": null
    },
    {
        "id": "test",
        "start_time": "2015/01/02",
        "end_time": "2015/06/01",
        "sampling": null
    }],
    "training_setup": {
        "training": {"data_split": "train",
                     "cross_validation": "false"},
        "tuning": {"data_split": "thresh-tune",
                   "cross_validation": "false"},
        "testing": {"data_split": "test",
                    "cross_validation": "false"}

    },
    "training_results": [{
        "random_seed": 0,
        "threshold": 0.210,
        "precision": 0.671,
        "recall": 0.918,
        "fpr": 0.102,
        "auc": 0.890,
        "feature_importances": ["SUM(Price - Cost)",
                                "STD(Price - Cost)"]

    },
    {
        "random_seed": 1,
        "threshold": 0.214,
        "precision": 0.702,
        "recall": 0.904,
        "fpr": 0.113,
        "auc": 0.892,
        "feature_importances": ["STD(Price - Cost)",
                                "SUM(Price - Cost)"]
    }
    ],
    "data_fields_used": {
            "orders": [{"name": "Price", "type": "float"},
                       {"name": "Cost", "type": "float"},
                       {"name": "Timestamp", "type": "datetime",
                        "format": "YY/mm/dd"},
                       {"name": "Product ID", "type": "str"},
                       {"name": "Order ID", "type": "str"}],
        "products": [{"name": "Category", "type": "str"},
                     {"name": "Product ID", "type": "str"}]
    }
}
